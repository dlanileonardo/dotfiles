{
  "state less widget": {
    "prefix": "statel",
     "body":  [
      "class ${1:name} extends StatelessWidget {",
      "  @override",
      "  Widget build(BuildContext context) {",
      "    return Container(",
      "      $2",
      "    );",
      "  }",
      "}"
    ],
    "description": "Widget Stateless"
  },
  "state full widget": {
    "prefix": "statef",
    "body": [
      "class ${1:name} extends StatefulWidget {",
      "  @override",
      "  _$1State createState() => _$1State();",
      "}",
      "",
      "class _$1State extends State<$1> {",
      "  @override",
      "  Widget build(BuildContext context) {",
      "    return Container(",
      "      $2",
      "    );",
      "  }",
      "}"
    ],
    "description": "Widget Statefull"
  }
