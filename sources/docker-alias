alias docker-kill-running='docker kill $(docker ps -q)'
alias docker-delete-all-images='docker rmi $(docker images -q)'
alias docker-cleanup-container='docker rm -v $(docker ps -a -q -f status=exited)'
alias docker-cleanup-images='docker rmi $(docker images -q -f dangling=true)'
alias docker-container-delete-stopped='docker rm $(docker ps -a -q)'
alias docker-portainer='docker run -d -p 9000:9000 -v ~/.portainer:/data -v "/var/run/docker.sock:/var/run/docker.sock" portainer/portainer && open http://127.0.0.1:9000'
alias docker-graphite='docker run -d --name graphite --restart=always -p 80:80 -p 2003-2004:2003-2004 -p 2023-2024:2023-2024 -p 8125:8125/udp -p 8126:8126 graphiteapp/graphite-statsd'
